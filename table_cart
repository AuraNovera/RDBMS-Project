CREATE TABLE CART(
ID INT,
NAME VARCHAR2(255),
PRICE FLOAT,
DISCOUNT FLOAT,
QUANTITY INT,
CATEGORY VARCHAR2(255),
FRANCHISE VARCHAR2(255),
CONSTRAINT FPK PRIMARY KEY(ID)
);


